 @model EduConnect.DTOs.AdministratorRequestModel   

<section id="main-content">
    <section class="wrapper">
        <h3><i class="fa fa-angle-right"></i>Register Form</h3>

        <div class="row mt">
            <div class="col-lg-12">
                <div class="form-panel">
                    <h4 class="mb"><i class="fa fa-angle-right"></i>Kindly register an administrator</h4>
                    <form asp-action="Create" class="form-horizontal tasi-form" enctype="multipart/form-data">
                        <div class="form-group has-success">
                            <label asp-for="FirstName" class="col-sm-2 control-label col-lg-2"
                                for="inputSuccess">FirstName</label>
                            <div class="col-lg-10">
                                <input asp-for="FirstName" type="text" class="form-control" id="inputSuccess"
                                    required />
                            </div>
                        </div>
                        <div class="form-group has-warning">
                            <label class="col-sm-2 control-label col-lg-2" for="inputWarning">LastName</label>
                            <div class="col-lg-10">
                                <input asp-for="LastName" type="text" class="form-control" id="inputWarning" required />
                            </div>
                        </div>
                        <div class="form-group has-error">
                            <label class="col-sm-2 control-label col-lg-2" for="inputError">Email</label>
                            <div class="col-lg-10">
                                <input asp-for="Email" type="text" class="form-control" id="inputError" required />
                            </div>
                        </div>
                         <div class="form-group has-error">
                            <label class="col-sm-2 control-label col-lg-2" for="inputError">Phone Number</label>
                            <div class="col-lg-10">
                                <input asp-for="PhoneNumber" type="number" class="form-control" id="inputError" required />
                            </div>
                        </div>
                        <div class="form-group has-error">
                            <label class="col-sm-2 control-label col-lg-2" for="inputError">Password</label>
                            <div class="col-lg-10">
                                <input asp-for="Password" type="text" class="form-control" id="inputError" required />
                            </div>
                        </div>
                        <div class="form-group has-error">
                            <label class="col-sm-2 control-label col-lg-2" for="inputError">Username</label>
                            <div class="col-lg-10">
                                <input asp-for="UserName" type="text" class="form-control" id="inputError" required />
                            </div>
                        </div>
                        <div class="form-group ">
                            <input type="submit" value="Submit"
                                class="btn btn-success trigger-btn col-sm-12" data-toggle="modal" />

                        </div>
                           <div class="btn btn-primary">
    <a asp-controller="User" asp-action="RedirectToDashBoard" style="color:white">Back to DashBoard</a>
     </div>v>
                    </form>
                </div>
            </div>
        </div>
    </section>
</section>